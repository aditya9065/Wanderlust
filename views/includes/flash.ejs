<%if(success.length > 0){%>
    <div class="col-6 offset-3 alert alert-success alert-dismissible fade show" role="alert">
        <%=success%>
        <%=error%>
        <button type="button" class="ms-auto btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
    </div>
<%}%> 

<%if(error.length > 0){%>
    <div class="col-6 offset-3 alert alert-danger alert-dismissible fade show" role="alert">
        <%=success%>
        <%=error%>
        <button type="button" class="ms-auto btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
    </div>
<%}%> 